# n8n - Archivo de ejemplo .env
# 🔐 Credenciales de acceso a N8N
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=secretpassword

# 🌐 Configuración de host y dominio
N8N_SECURE_COOKIE=true
N8N_HOST=https://n8n.tudominio.com
N8N_PROTOCOL=https
N8N_PORT=5678
WEBHOOK_URL=https://n8n.tudominio.com/

# 🗄️ Base de datos Postgres para N8N
N8N_DB_USER=n8n
N8N_DB_PASSWORD=secretpassword
N8N_DB_NAME=n8n

# 🕒 Zona horaria
GENERIC_TIMEZONE=Europe/Madrid